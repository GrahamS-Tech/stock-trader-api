<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="StockTraderAPI"
                   namespace="StockTraderAPI.Adapters">
  <class name="watch_list" table="watch_list" lazy="true">
    <id name="Id" column="id" unsaved-value="0" generator="native">
    </id>
    <many-to-one name="ProfileId" class="profile">
      <column name="profile_id" not-null="true"/>
    </many-to-one>
    <property name="Ticker" column="ticker"/>
    <property name="DateAdded" column="date_added"/>
  </class>
</hibernate-mapping>